@book{Anonymous:2005whb,
  title = {Pollination Ecology and the Rain Forest: {{Sarawak}} Studies},
  year = {2005},
  series = {Ecological Studies},
  volume = {174},
  abstract = {This 222-page book, entitled 'Pollination Ecology and the Rain Forest: Sarawak Studies' is volume 174 in the series Ecological Studies. The ground-breaking canopy access and rain forest research at Lambir Hills National Park in Sarawak, Malaysia, has contributed an immense body of knowledge and its major studies are synthesized here for the first time. The focus of this volume is on plant-animal interactions and some of the foundations that create and maintain tropical diversity, especially pollination and the phenomenon of general flowering. The work discussed here has implications for tropical biology, ecology and pollination studies. The book contains 18 individually-authored chapters written in English. Areas covered in these chapters include rarity and pollination in rain forests, the Canopy Biology Program in Sarawak, soil-related floristic variation in a hyperdiverse Dipterocarp forest, plant reproductive phenology and general flowering in a mixed Dipterocarp forest, a severe drought in Lambir Hills National Park, the plant-pollinator community in a lowland Dipterocarp forest, floral resource utilization by stingless bees, honeybees in Borneo, beetle pollination in tropical rain forests, an overview of a diverse plant assemblage, the ecology of trampolining bees and understorey pollinators, vertebrate-pollinated plants, insect predators of Dipterocarp seeds, diversity of anti-herbivore defenses in Macaranga, coevolution of ants and plants, the lowland tropical rain forests of Asia and America with respect to parallels, convergence and divergence, plant diversity in the Lambir Hills, and toward tropical rain forest conservation. The power of the El Nino-Southern Oscillation events and drought are discussed in detail particularly in their effects on mutualisms. The book highlights a list of contributors and their respective institutions and contains 76 illustrations, 12 of which are in full color. The last chapter of the book is followed by appendices, a glossary, a bibliography and an index. The work discussed in this volume will be of interest to researchers in tropical biology, ecology and pollination studies.},
  date-added = {2016-02-09T09:57:54GMT},
  date-modified = {2016-02-09T16:43:10GMT},
  rating = {0},
  uri = {papers3://publication/uuid/30DA97FD-0D65-43F7-8690-E3CF80EC7D4D}
}

