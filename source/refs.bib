@article{Southwood:2006cm,
  title = {Observations on Related Ecological Exponents},
  author = {Southwood, T Richard E and May, Robert M and Sugihara, George},
  year = {2006},
  journal = {Proceedings of the National Academy of Sciences of the United States of America},
  volume = {103},
  pages = {6931--6933},
  publisher = {{NATL ACAD SCIENCES}},
  address = {{2101 CONSTITUTION AVE NW, WASHINGTON, DC 20418 USA}},
  doi = {DOI 10.1073/pnas.0600988103},
  abstract = {We observe a relationship among three independently derived power laws in ecology: (i) total number of species versus area, (ii) species frequency versus species length, and (iii) maximal body size versus area. Aside from showing how these historically disparate phenomena are connected, we show how recent empirical results relating the maximal body size of top terrestrial vertebrates to the square root of land area conform to a prior theoretical expectation given by two of the above power laws. Of particular interest is the observation that the exponent relating species length to species frequency suggests a dimension for niche space for terrestrial vertebrate assemblages of D approximate to 3/2. This value, along with power law for maximal body size, versus area, gives rise to the canonical species area exponent z approximate to 1/4.},
  date-added = {2012-04-02T11:04:48GMT},
  date-modified = {2018-05-21T13:35:57GMT},
  local-url = {file://localhost/Users/dorme/References/Library.papers3/Articles/2006/Southwood/PNAS\%202006\%20Southwood.pdf},
  rating = {0},
  file = {/Users/dorme/Zotero/storage/CI2453JA/PNAS 2006 Southwood.pdf}
}

@misc{Spatialaggregation:2019tk,
  title = {Spatial Aggregation and Seedling Survival of the {{Borneo Ironwood}} ({{Eusideroxylon}} Zwageri)},
  author = {Qie, Lan and Ewers, Rob and Brisbane, Jeanelle and Elsy, Alexander and Ettinger, Maisie and Kwasnicka, Magdalena and Majcher, Bartosz and Ow, Siew Ngim and Peel, Anna and Robertson, Alasdair and Sawyer, Amber and Sebastianelli, Matteo and Stumvoll, Ashley and Sulivan, Joseph and Vigus, Hannah and Lui, Yan Nin and Duan, Muying and Pounsin, Grace and Nilus, Reuben},
  year = {2019},
  publisher = {{Zenodo}},
  abstract = {\textexclamdown b\textquestiondown Description: \textexclamdown/b\textquestiondown This dataset was collected by the Imperial College London MRes Tropical Forest Ecology Field Course as a group project in Maliau Basin. Fieldwork was carried out on 29 Jan - 1 Feb 2018. A 4-ha plot was set up to assess the spatial aggregation of the Borneo Ironwood (Eusideroxylon zwageri, local name Belian) and seedling survival. All trees with DBH  \&gt;1cm were mapped with XY coordinate rounded to nearest 1m. DBH class was rounded down to the nearest 10cm, e.g. size 10, 20, 30 etc. Size class 1 refers to trees with DBH between 1 and 10cm. A swamp area on the north and northeast of \textasciitilde 1-ha was not assessible hence data defficient, but as Belian is not known to growth in water logged conditions the species was presumed to be absent in this swamp area. Belian seedlings were surveyed in 540 5x5m subplots in the centre zone of the 4-ha plot. In each seedling plot, healthy and damaged (primarily herbivory damage by mammals, defined as damaged when top shoot, i.e. apical meristem, was missing). Other variables recorded in seedling plots include slope angle and ground vegetation cover.\textexclamdown b\textquestiondown Project: \textexclamdown/b\textquestiondown This dataset was collected as part of the following SAFE research project: \textexclamdown b\textquestiondown MRes Tropical Forest Ecology Field Course\textexclamdown/b\textquestiondown\textexclamdown b\textquestiondown Funding: \textexclamdown/b\textquestiondown These data were collected as part of research funded by: Natural Environment Research Council (Directed grant, NE/P00363X/1, https://gtr.ukri.org/projects?ref=NE\%2FP00363X\%2F1)Imperial College London (MRes Tropical Forest Ecology field course program)This dataset is released under the CC-BY 4.0 licence, requiring that you cite the dataset in any outputs, but has the additional condition that you acknowledge the contribution of these funders in any outputs.\textexclamdown b\textquestiondown XML metadata: \textexclamdown/b\textquestiondown GEMINI compliant metadata for this dataset is available here\textexclamdown b\textquestiondown Files: \textexclamdown/b\textquestiondown This consists of 1 file: Belian\_data\_for\_SAFE\_upload\_LQ\_10Jan\_2019.xlsx\textexclamdown b\textquestiondown Belian\_data\_for\_SAFE\_upload\_LQ\_10Jan\_2019.xlsx\textexclamdown/b\textquestiondown This file contains dataset metadata and 2 data tables:\textexclamdown b\textquestiondown Belian tree coordinates\textexclamdown/b\textquestiondown{} (described in worksheet Belian trees)Description: All trees with DBH  \&gt;1cm were mapped in a 4-ha plot with XY coordinate rounded to nearest 1m. Each row corresponds to a Belian tree. DBH class was rounded down to the nearest 10cm, e.g. size 10, 20, 30 etc. Size class 1 refers to trees with DBH between 1 and 10cm. A swamp area on the north and northeast of \textasciitilde 1-ha was not assessible hence data defficient, but as Belian is not known to growth in water logged conditions the species was presumed to be absent in this swamp area.Number of fields: 9Number of data rows: 91Fields: \textexclamdown b\textquestiondown X10\textexclamdown/b\textquestiondown : X coordinate of the southwest corner of 10x10 m subplot (Field type: Numeric)\textexclamdown b\textquestiondown Y10\textexclamdown/b\textquestiondown : Y coordinate of the southwest corner of 10x10 m subplot (Field type: Numeric)\textexclamdown b\textquestiondown X\textexclamdown/b\textquestiondown : X coordinate within the 10x10m subplot (local X) (Field type: Numeric)\textexclamdown b\textquestiondown Y\textexclamdown/b\textquestiondown : Y coordinate within the 10x10m subplot (local Y) (Field type: Numeric)\textexclamdown b\textquestiondown class\textexclamdown/b\textquestiondown : DBH rounded down to the nearest 10cm, e.g. size class 10, 20, 30 etc. Size class 1 refers to trees with DBH between 1 and 10cm (Field type: Numeric Trait)\textexclamdown b\textquestiondown remark\textexclamdown/b\textquestiondown : remark on whether tree is a resprout from the base of an older tree and other tree condition information (Field type: Comments)\textexclamdown b\textquestiondown collected\_by\textexclamdown/b\textquestiondown : members of field teams: 4 groups of 4 students each (Field type: Comments)\textexclamdown b\textquestiondown X\_coord\textexclamdown/b\textquestiondown : X coordinate within the 4-ha plot (global X) (Field type: Numeric)\textexclamdown b\textquestiondown Y\_coord\textexclamdown/b\textquestiondown : Y coordinate within the 4-ha plot (global Y) (Field type: Numeric)\textexclamdown b\textquestiondown Belian seedling plots\textexclamdown/b\textquestiondown{} (described in worksheet Belian seedlings)Description: Belian seedlings were surveyed in 540 5x5m subplots in the centre zone of the 4-ha plot. Each row corresponds to a seedling plot. In each seedling plot, healthy and damaged (primarily herbivory damage by mammals, defined as damaged when top shoot, i.e. apical meristem, was missing). Other variables recorded in seedling plots include slope angle and ground vegetation cover.Number of fields: 10Number of data rows: 540Fields: \textexclamdown b\textquestiondown X10\textexclamdown/b\textquestiondown : X coordinate of the southwest corner of 10x10 m subplot (Field type: Numeric)\textexclamdown b\textquestiondown Y10\textexclamdown/b\textquestiondown : Y coordinate of the southwest corner of 10x10 m subplot (Field type: Numeric)\textexclamdown b\textquestiondown healthy\textexclamdown/b\textquestiondown : count of healthy Belian seedlings within 5x5m plot. "healthy" is defined as opposed to "damaged" (see next data column) (Field type: Abundance)\textexclamdown b\textquestiondown damaged\textexclamdown/b\textquestiondown : count of damaged Belian seedlings within 5x5m plot. "damaged" is defined as when a seedling suffered severe herbivory damage (including dead seedlings) with the apical meristem entirely missing (eaten by mammals) (Field type: Abundance)\textexclamdown b\textquestiondown slope\textexclamdown/b\textquestiondown : mean slope angle for each seedling subplot, a measure of habitat topography (Field type: Numeric)\textexclamdown b\textquestiondown veg\_cover\textexclamdown/b\textquestiondown : ground vegetation cover for each subplot, a measure of habitat and interspecific competition (Field type: Ordered Categorical)\textexclamdown b\textquestiondown cache\textexclamdown/b\textquestiondown : when seedlings occur in dense clusters especially under fallen tree or in buttress crevices, this may indicate they were deposited there by rodents, i.e. caching (Field type: Comments)\textexclamdown b\textquestiondown remark\textexclamdown/b\textquestiondown : remark on habitat. seedling plots on footpath or water should be excluded from data analysis (Field type: Comments)\textexclamdown b\textquestiondown collected\_by\textexclamdown/b\textquestiondown : members of field teams: 4 groups of 4 students each (Field type: Comments)\textexclamdown b\textquestiondown total\textexclamdown/b\textquestiondown : total number of Belian seedlings (Field type: Abundance)\textexclamdown b\textquestiondown Date range: \textexclamdown/b\textquestiondown 2018-01-29 to 2018-02-01\textexclamdown b\textquestiondown Latitudinal extent: \textexclamdown/b\textquestiondown 4.7383 to 4.7383\textexclamdown b\textquestiondown Longitudinal extent: \textexclamdown/b\textquestiondown 116.9713 to 116.9713\textexclamdown b\textquestiondown Taxonomic coverage: \textexclamdown/b\textquestiondown\textexclamdown br\textquestiondown{} All taxon names are validated against the GBIF backbone taxonomy. If a dataset uses a synonym, the accepted usage is shown followed by the dataset usage in brackets. Taxa that cannot be validated, including new species and other unknown taxa, morphospecies, functional groups and taxonomic levels not used in the GBIF backbone are shown in square brackets.Plantae\textexclamdown br\textquestiondown{} - Tracheophyta\textexclamdown br\textquestiondown{} - - Magnoliopsida\textexclamdown br\textquestiondown{} - - - Laurales\textexclamdown br\textquestiondown{} - - - - Lauraceae\textexclamdown br\textquestiondown{} - - - - - \textexclamdown i\textquestiondown Eusideroxylon\textexclamdown/i\textquestiondown\textexclamdown br\textquestiondown{} - - - - - - \textexclamdown i\textquestiondown Eusideroxylon zwageri\textexclamdown/i\textquestiondown\textexclamdown br\textquestiondown},
