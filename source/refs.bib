@article{Pigot:2012bk,
  title = {Speciation and Extinction Drive the Appearance of Directional Range Size Evolution in Phylogenies and the Fossil Record},
  author = {Pigot, Alex L. and Owens, Ian P. F. and Orme, C. David L.},
  year = {2012},
  month = feb,
  journal = {PLoS Biology},
  volume = {10},
  number = {2},
  pages = {e1001260},
  publisher = {{Public Library of Science}},
  doi = {10.1371/journal.pbio.1001260},
  abstract = {While the geographic range of a species is a fundamental unit of macroecology and a leading predictor of extinction risk, the evolutionary dynamics of species' ranges remain poorly understood. Based on statistical associations between range size and species age, many studies have claimed support for general models of range evolution in which the area occupied by a species varies predictably over the course of its life. Such claims have been made using both paleontological data and molecular estimates of the age of extant species. However, using a stochastic model, we show that the appearance of trends in range size with species' age can arise even when range sizes have evolved at random through time. This occurs because the samples of species used in existing studies are likely to be biased with respect to range size: for example, only those species that happened to have large or expanding ranges are likely to survive to the present, while extinct species will tend to be those whose ranges, by chance, declined through time. We compared the relationship between the age and range size of species arising under our stochastic model to those observed across 1,269 species of extant birds and mammals and 140 species of extinct Cenozoic marine mollusks. We find that the stochastic model is able to generate the full spectrum of empirical age\textendash area relationships, implying that such trends cannot be simply interpreted as evidence for models of directional range size evolution. Our results therefore challenge the theory that species undergo predictable phases of geographic expansion and contraction through time.},
  date-added = {2012-04-02T11:03:46GMT},
  date-modified = {2015-07-01T08:53:25GMT},
  local-url = {file://localhost/Users/dorme/References/Library.papers3/Articles/2012/Pigot/PLoS\%20Biology\%202012\%20Pigot.pdf},
  rating = {0},
  uri = {papers3://publication/doi/10.1371/journal.pbio.1001260},
  file = {/Users/dorme/Zotero/storage/SKN8EZNL/PLoS Biology 2012 Pigot.pdf}
}

