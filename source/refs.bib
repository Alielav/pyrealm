@article{Alroy:1998th,
  title = {Cope's Rule and the Dynamics of Body Mass Evolution in {{North American}} Fossil Mammals},
  author = {Alroy, J},
  year = {1998},
  journal = {Science (New York, N.Y.)},
  volume = {280},
  pages = {731--734},
  abstract = {Body mass estimates for 1534 North American fossil mammal species show that new species are on average 9.1\% larger than older species in the same genera. This within-lineage effect is not a sampling bias. It persists throughout the Cenozoic; accounting for the gradual overall increase in average mass (Cope's rule). The effect is stronger for larger mammals, being near zero for small mammals. This variation partially explains the unwavering lower size limit and the gradually expanding mid-sized gap, but not the sudden large increase in the upper size limit, at the Cretaceous-Tertiary boundary.},
  date-added = {2012-04-02T11:04:44GMT},
  date-modified = {2015-07-01T08:53:03GMT},
  local-url = {file://localhost/Users/dorme/References/Library.papers3/Articles/1998/Alroy/Science\%201998\%20Alroy.pdf},
  rating = {0},
  uri = {papers3://publication/uuid/91C9502C-5F25-493E-AE93-137D00C3F049},
  file = {/Users/dorme/Zotero/storage/ERYBNC3F/Science 1998 Alroy.pdf}
}

@article{Alroy:2001tb,
  title = {Effects of Sampling Standardization on Estimates of {{Phanerozoic}} Marine Diversification},
  author = {Alroy, J and Marshall, CR and Bambach, RK and Bezusko, K and Foote, M and Fursich, FT and Hansen, TA and Holland, SM and Ivany, LC and Jablonski, D and Jacobs, DK and Jones, DC and Kosnik, MA and Lidgard, S and Low, S and Miller, AI and {Novack-Gottshall}, PM and Olszewski, TD and Patzkowsky, ME and Raup, DM and Roy, K and Sepkoski, JJ and Sommers, MG and Wagner, PJ and Webber, A},
  year = {2001},
  journal = {Proceedings of the National Academy of Sciences of the United States of America},
  volume = {98},
  pages = {6261--6266},
  abstract = {Global diversity curves reflect more than just the number of taxa that have existed through time: they also mirror variation in the nature of the fossil record and the way the record is reported. These sampling effects are best quantified by assembling and analyzing large numbers of locality-specific biotic inventories. Here, we introduce a new database of this kind for the Phanerozoic fossil record of marine invertebrates. We apply four substantially distinct analytical methods that estimate taxonomic diversity by quantifying and correcting for variation through time in the number and nature of inventories. Variation introduced by the use of two dramatically different counting protocols also is explored. We present sampling-standardized diversity estimates for two long intervals that sum to 300 Myr (Middle Ordovician-Carboniferous; Late Jurassic-Paleogene), Our new curves differ considerably from traditional, synoptic curves. For example, some of them imply unexpectedly low late Cretaceous and early Tertiary diversity levels. However, such factors as the current emphasis in the database on North America and Europe still obscure our view of the global history of marine biodiversity. These limitations will be addressed as the database and methods are refined.},
  date-added = {2012-04-02T11:04:48GMT},
  date-modified = {2018-05-21T13:35:57GMT},
  local-url = {file://localhost/Users/dorme/References/Library.papers3/Articles/2001/Alroy/PNAS\%202001\%20Alroy.pdf},
  rating = {0},
  uri = {papers3://publication/uuid/D394EB79-9390-406A-A657-2062773F2A84},
  file = {/Users/dorme/Zotero/storage/LLNFWIHE/PNAS 2001 Alroy.pdf}
}

@article{Alsos:2007ee,
  title = {Frequent Long-Distance Plant Colonization in the Changing {{Arctic}}},
  author = {Alsos, Inger Greve and Eidesen, Pernille Bronken and Ehrich, Dorothee and Skrede, Inger and Westergaard, Kristine and Jacobsen, Gro Hilde and Landvik, Jon Y. and Taberlet, Pierre and Brochmann, Christian},
  year = {2007},
  journal = {Science (New York, N.Y.)},
  volume = {316},
  pages = {1606--1609},
  doi = {DOI 10.1126/science.1139178},
  abstract = {The ability of species to track their ecological niche after climate change is a major source of uncertainty in predicting their future distribution. By analyzing DNA fingerprinting ( amplified fragment-length polymorphism) of nine plant species, we show that long-distance colonization of a remote arctic archipelago, Svalbard, has occurred repeatedly and from several source regions. Propagules are likely carried by wind and drifting sea ice. The genetic effect of restricted colonization was strongly correlated with the temperature requirements of the species, indicating that establishment limits distribution more than dispersal. Thus, it may be appropriate to assume unlimited dispersal when predicting long-term range shifts in the Arctic.},
  date-added = {2012-04-02T11:04:47GMT},
  date-modified = {2015-07-01T08:53:02GMT},
  local-url = {file://localhost/Users/dorme/References/Library.papers3/Articles/2007/Alsos/Science\%202007\%20Alsos.pdf},
  rating = {0},
  file = {/Users/dorme/Zotero/storage/G5NKV8ET/Science 2007 Alsos.pdf}
}

@misc{Alternativestreamn:2019vi,
  title = {Alternative Stream Network Data for the {{SAFE Project}}},
  author = {Orme, C. David L.},
  year = {2019},
  publisher = {{Zenodo}},
  abstract = {\textexclamdown b\textquestiondown Description: \textexclamdown/b\textquestiondown This zipfile contains three shapefiles of linear geometries showing channel networks across the SAFE landscape.\textexclamdown br\textquestiondown\textexclamdown br\textquestiondown RIVERS\_UTM.shp: From Igor Lysenko's SAFE pack, this is a river network with two areas, covering Maliau and the area around the SAFE experimental region. The network density is much higher in Maliau than SAFE and the channel network around SAFE is not complete. This is projected in UTM50N WGS84.\textexclamdown br\textquestiondown LFEriver\_SL.shp: This is provided (by Sarah Luke?) through Clare Wilkinson's GIS files and adds a critical missing stream for the Logged Forest Edge (LFE) catchment. This is projected in UTM50N WGS84.\textexclamdown br\textquestiondown all\_rivers.shp: This is provided through Clare Wilkinson's GIS files and provides streams for a single region covering Danum and the SAFE experimental region but not sampled watersheds in Oil Palm plantations to the south of SAFE. The original file is missing projection information (no .prj file) but other files in the same dataset are projected in RSO Timbalai 1948 and using this projection fits with the context of other data. The version uploaded onto Zenodo has been reprojected into UTM50N WGS84.\textexclamdown br\textquestiondown\textexclamdown br\textquestiondown\textexclamdown br\textquestiondown Although the files are not consistent, they do contain channels and channel data that are referenced in some studies. The provenance of these files are unknown, although the following suggests that they may be traced using GPS or from imagery:\textexclamdown br\textquestiondown\textexclamdown br\textquestiondown Incomplete coverage within the regions they cover.\textexclamdown br\textquestiondown Treatment of larger rivers, changing from a single line feature showing the stream centreline (?) to double lines showing the river banks.\textexclamdown br\textquestiondown Variation in network density: the western edge of all\_rivers.shp shows a vertical band about 4 km wide of higher stream density than the rest of the region; RIVERS\_UTM.shp shows marked differences in stream density between SAFE and Maliau.\textexclamdown br\textquestiondown\textexclamdown b\textquestiondown Project: \textexclamdown/b\textquestiondown This dataset was collected as part of the following SAFE research project: \textexclamdown b\textquestiondown SAFE CORE DATA\textexclamdown/b\textquestiondown\textexclamdown b\textquestiondown XML metadata: \textexclamdown/b\textquestiondown GEMINI compliant metadata for this dataset is available here\textexclamdown b\textquestiondown Files: \textexclamdown/b\textquestiondown This dataset consists of 2 files: SAFE\_Alternative\_Stream\_network\_metadata.xlsx, Preexisting\_SAFE\_river\_files.zip\textexclamdown b\textquestiondown SAFE\_Alternative\_Stream\_network\_metadata.xlsx\textexclamdown/b\textquestiondown This file only contains metadata for the files below\textexclamdown b\textquestiondown Preexisting\_SAFE\_river\_files.zip\textexclamdown/b\textquestiondown Description: Contains three shapefiles of channel networks.This file contains 3 data tables:\textexclamdown b\textquestiondown Feature properties\textexclamdown/b\textquestiondown{} (described in worksheet LFEriver\_SL)Description: Field descriptions for shapefile propertiesNumber of fields: 1Number of data rows: Unavailable (table metadata description only).Fields: \textexclamdown b\textquestiondown Id\textexclamdown/b\textquestiondown : Identity of river (Field type: id)\textexclamdown b\textquestiondown Feature properties\textexclamdown/b\textquestiondown{} (described in worksheet RIVERS\_UTM)Description: Field descriptions for shapefile propertiesNumber of fields: 1Number of data rows: Unavailable (table metadata description only).Fields: \textexclamdown b\textquestiondown NAME\textexclamdown/b\textquestiondown : Local na