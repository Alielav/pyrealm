@article{Bolker:2009jr,
  title = {Generalized Linear Mixed Models: A Practical Guide for Ecology and Evolution},
  author = {Ben Bolker, Benjamin M. and Brooks, Mollie E. and Clark, Connie J. and Geange, Shane W. and Poulsen, John R. and Stevens, M. Henry H. and White, Jada-Simone S.},
