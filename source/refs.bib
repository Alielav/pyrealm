@article{Cooper:2011gq,
  title = {Phylogenetic Conservatism of Environmental Niches in Mammals},
  author = {Cooper, Natalie and Freckleton, Rob P. and Jetz, Walter},
  year = {2011},
  month = jan,
  journal = {Proceedings of the Royal Society Series B - Biological Sciences},
  volume = {111},
  number = {1},
  pages = {1--12},
  doi = {10.1098/rspb.2010.2207},
  abstract = {Phylogenetic niche conservatism is the pattern where close relatives occupy similar niches, whereas distant relatives are more dissimilar. We suggest that niche conservatism will vary across clades in relation to their characteristics. Specifically, we investigate how conservatism of environmental niches varies among mammals according to their latitude, range size, body size and specialization. We use the Brownian rate parameter, {$\sigma$}(2), to measure the rate of evolution in key variables related to the ecological niche and define the more conserved group as the one with the slower rate of evolution. We find that tropical, small-ranged and specialized mammals have more conserved thermal niches than temperate, large-ranged or generalized mammals. Partitioning niche conservatism into its spatial and phylogenetic components, we find that spatial effects on niche variables are generally greater than phylogenetic effects. This suggests that recent evolution and dispersal have more influence on species' niches than more distant evolutionary events. These results have implications for our understanding of the role of niche conservatism in species richness patterns and for gauging the potential for species to adapt to global change.},
  date-added = {2012-04-02T11:04:40GMT},
  date-modified = {2015-07-01T08:53:20GMT},
  local-url = {file://localhost/Users/dorme/References/Library.papers3/Articles/2011/Cooper/Proc\%20Roy\%20Soc\%20B\%202011\%20Cooper.pdf},
  pmid = {21208967},
  rating = {0},
  uri = {papers3://publication/doi/10.1098/rspb.2010.2207},
  file = {/Users/dorme/Zotero/storage/TGZKXCYA/Proc Roy Soc B 2011 Cooper.pdf}
}

@misc{Coreinvertbiomas:2019uu,
  title = {Core - Invert Biomass + Ordinal Sort},
  author = {Sawang, Anati and Sharp, Adam and Chung, Arthur and Ewers, Robert M and Barclay, Max},
  year = {2019},
  publisher = {{Zenodo}},
  abstract = {\textexclamdown b\textquestiondown Description: \textexclamdown/b\textquestiondown The Core Insect Pitfall/Malaise Trapping program has been running since 2012. The novel method was first developed by Robert M Ewers. This provides not only insect specimens, but other invertebrates such as myriapods, collembola, arachnids, and occasionally provides vertebrates such as small rodents, snakes, and amphibians. All specimens and data is kept as part of the Core of the SAFE project. The majority of the data which has been exploited so far remains with the Coleoptera and Staphylinidae groups, while much of the preliminary field iding groupings are currently not used, the data remains to have serious as of yet untapped potential for future workers.\textexclamdown br\textquestiondown Trap construction:\textexclamdown br\textquestiondown Traps were based on a design combining pitfall, flight-interception, and malaise traps. Flying insects were directed either upwards into a "top" trap or downwards into a "bottom" trap. \textexclamdown br\textquestiondown Components used in the construction of each trap are as follows:\textexclamdown br\textquestiondown\textbullet 1 25cm diameter, 20cm depth, 4.5cm spout aperture Blue Plastic "top" funnel.\textexclamdown br\textquestiondown\textbullet 1 20cm diameter, 20cm depth, 2cm spout aperture Blue Plastic "bottom" funnel.\textexclamdown br\textquestiondown\textbullet 1 Xcm four pointed star cloth, acting as a malaise tent "director", held at approximately 90 degrees to the trap with clear fishing line.\textexclamdown br\textquestiondown\textbullet 1 Xcm diameter, 90 degree grey plastic elbow pipe, one end modified to include appropriate teeth to screw collection bottle onto which.\textexclamdown br\textquestiondown\textbullet 1 Xml "bottom" collection bottle, modified with mesh lined holes to allow water to escape to prevent overspilling.\textexclamdown br\textquestiondown\textbullet 1 Xml "top" collection bottle, unmodified.\textexclamdown br\textquestiondown\textbullet 1 77cm length cross-intersecting clear plastic (PVC) flight interception vane, supported by 4 (metal) 68.5cm aluminum poles.\textexclamdown br\textquestiondown\textbullet 8 white plastic Zip-Ties.\textexclamdown br\textquestiondown\textbullet 70\% ethanol solution to fill the collection bottles.\textexclamdown br\textquestiondown Trapping strategy:\textexclamdown br\textquestiondown The pitfall style bottom trap are dug flush into the ground where possible and the hole preserved between trapping periods to limit catch bias associated with soil and leaf litter disturbance (Digweed et al. 1995). Once constructed, and occasion start time recorded, the traps are left for three days before collection with collection time also recorded.\textexclamdown br\textquestiondown Samples are then stored in a chest freezer at -10 degC, before being taken to Maliau Basin research station and sorted by order and 70\% ethanol before again being stored in freezers.\textexclamdown br\textquestiondown Justifications:\textexclamdown br\textquestiondown This combination of traps is implemented to target invertebrates of various morphology and behaviour to take full advantage of the sampling opportunity.\textexclamdown br\textquestiondown\textexclamdown b\textquestiondown Project: \textexclamdown/b\textquestiondown This dataset was collected as part of the following SAFE research project: \textexclamdown b\textquestiondown Spatial scaling of beetle community diversity\textexclamdown/b\textquestiondown\textexclamdown b\textquestiondown Funding: \textexclamdown/b\textquestiondown These data were collected as part of research funded by: Sime Darby (Standard grant, SAFE - core data, na)This dataset is released under the CC-BY 4.0 licence, requiring that you cite the dataset in any outputs, but has the additional condition that you acknowledge the contribution of these funders in any outputs.\textexclamdown b\textquestiondown Permits: \textexclamdown/b\textquestiondown These data were collected under permit from the following authorities:Sabah Biodiversity Council (Research licence na)\textexclamdown b\textquestiondown XML metadata: \textexclamdown/b\textquestiondown GEMINI compliant metadata for this dataset is available here\textexclamdown b\textquestiondown Files: \textexclamdown/b\textquestiondown This consists of 1 file: Core\_insect\_2011\_2012\_2017.xlsx\textexclamdown b\textquestiondown Core\_insect\_2011\_2012\_2017.xlsx\textexclamdown/b\textquestiondown This file contains dataset metadata and 2 data tables:\textexclamdown b\textquestiondown Insect sorting\textexclamdown/b\textquestiondown{} (described in worksheet Insect\_sorting)Description: Insect sorting 2011, 2012 and 2017Number of fields: 28Number of data rows: 2502Fields: \textexclamdown b\textquestiondown TrapNo\textexclamdown/b\textquestiondown : Trap number (Field type: id)\textexclamdown b\textquestiondown Fragment\textexclamdown/b\textquestiondown : Fragment number in the SAFE landscape (Field type: id)\textexclamdown b\textquestiondown Plot\textexclamdown/b\textquestiondown : Insect rapping point (Field type: location)\textexclamdown b\textquestiondown Top\_Bottom\textexclamdown/b\textquestiondown : Position in the trap (Field type: categorical)\textexclamdown b\textquestiondown DateColl\textexclamdown/b\textquestiondown : Date the trap was collected (Field type: date)\textexclamdown b\textquestiondown TimeColl\textexclamdown/b\textquestiondown : Time the trap was collected (Field type: time)\textexclamdown b\textquestiondown Coleoptera\textexclamdown/b\textquestiondown : Coleoptera abundance (Field type: abundance)\textexclamdown b\textquestiondown Staphylinids\textexclamdown/b\textquestiondown : Staphylinids abundance (Field type: abundance)\textexclamdown b\textquestiondown Formicidae\textexclamdown/b\textquestiondown : Formicidae abundance (Field type: abundance)\textexclamdown b\textquestiondown Isoptera\textexclamdown/b\textquestio