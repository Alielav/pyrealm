@article{Fauvelot:2005wa,
  title = {Isolation and Characterization of Microsatellites in Two Tropical Butterflies, {{Drupadia}} Theda and {{Arhopala}} Epimuta ({{Lepidoptera}} : {{Lycaenidae}})},
  author = {Fauvelot, C},
  year = {2005},
  journal = {MOLECULAR ECOLOGY NOTES},
  volume = {5},
  number = {4},
  pages = {724--726},
  abstract = {Little is known about the effect of El Nino Southern Oscillation-induced fires on the genetic diversity of tropical rainforest species. Here, I report on the isolation and characterization of 10 microsatellite loci, five loci each, for two lycaenid butterfly species in East Kalimantan, Indonesia, namely Drupadia theda and Arhopala epimuta, which will be used to specifically study the impact of disturbance on genetic diversity. Microsatellite enrichment was carried out using streptavidin-coated magnetic beads. Positive colonies were identified with the three-primer polymerase chain reaction (PIMA). Cross-species amplifications conducted both within and between genera were successful in 16 out of 20 tests.},
  date-added = {2016-02-09T09:57:55GMT},
  date-modified = {2016-02-09T09:58:19GMT},
  rating = {0},
  uri = {papers3://publication/uuid/172ECD2F-8DF1-4DED-9F80-B76B7EC70614}
}

@article{Fauvelot:2006th,
  title = {Short-Term Impact of Disturbance on Genetic Diversity and Structure of {{Indonesian}} Populations of the Butterfly {{Drupadia}} Theda in {{East Kalimantan}}},
  author = {Fauvelot, C and Cleary, D F R and Menken, S B J},
  year = {2006},
  journal = {Molecular Ecology},
  volume = {15},
  number = {8},
  pages = {2069--2081},
  abstract = {We investigated the short-term impact of disturbance on genetic diversity and structure of the tropical butterfly Drupadia theda Felder (Lepidoptera: Lycaenidae). Populations were sampled from five landscapes in East Kalimantan (Borneo, Indonesia) which were differentially disturbed by selective logging and the 1997/1998 El Nino Southern Oscillation (ENSO)-induced drought and fires. Sampling occurred before (in 1997) and after the forest fires (in 1998, 1999, 2000, and 2004). Drupadia theda populations underwent serious population size reductions following the 1997/1998 ENSO event. For a total of 208 individuals, we sequenced a 509-bp segment of mtDNA containing the control region plus the 5' end of the 12S rDNA gene. Haplotype diversity in D. theda populations ranged from 0.468 to 0.953. Just after the 1997/1998 ENSO event, number of recorded individuals and genetic diversity were very low in D. theda populations sampled in the two severely burned areas and in a small pristine forest fragment that was surrounded by burned forest and thereby affected by drought. Interestingly, higher levels of genetic diversity were observed in logged forest compared to proximate pristine forest. After 1998, the genetic composition within the three ENSO-disturbed areas diverged. In the twice-burned forest, the genetic diversity in 1999 already approached pre-fire levels, while it remained nearly unchanged in proximate once-burned forest. Our data suggest that the 1997/1998 ENSO-induced drought and fires caused massive reductions in the genetic diversity of D. theda and that population recoveries were linked to their geographical position relative to patches of unburned forest (and thus to source populations).},
  date-added = {2016-02-09T09:57:54GMT},
  date-modified = {2016-02-09T09:58:19GMT},
  rating = {0},
  uri = {papers3://publication/uuid/0D00313F-B232-4BC8-B660-66F66BC1199C}
}

@article{Fauvelot:2006uy,
  title = {Short-Term Impact of 1997/1998 {{ENSO-induced}} Disturbance on Abundance and Genetic Variation in a Tropical Butterfly},
  author = {Fauvelot, Cecile and Cleary, Daniel F R and Menken, Steph B J},
  year = {2006},
  journal = {JOURNAL OF HEREDITY},
  volume = {97},
  number = {4},
  pages = {367--380},
  abstract = {In order to assess the short-term impact of habitat loss after disturbance, we studied Arhopala epimuta (Lepidoptera: Lycaenidae) populations in 5 landscapes in Borneo that were differentially affected by the 1997/1998 El Nino Southern Oscillation-induced drought and fire. Sampling was conducted before (1997) and after (1998 and 2000) disturbance. This study combined demographic and genetic data inferred from the analysis of 5 microsatellite loci and mitochondrial DNA (mtDNA) control region sequences. Over all 5 landscapes, a total of 313 A. epimuta were sampled over the 3-year survey. Butterfly abundance varied greatly both spatially and temporally (within disturbed landscapes). After the disturbance, a 4-fold population expansion was observed in a small unburned isolate, whereas population extinction was observed in one of the severely burned areas. The analysis of mtDNA sequences in a subsample of 106 A. epimuta revealed no significant spatial or temporal genetic structure. The analysis of 5 microsatellite loci revealed high frequencies of null alleles. Genetic evidence of recent change in population size was found in all 3 unburned landscapes using microsatellites. Congruent to mtDNA, microsatellites failed to detect significant genetic structure according to sampling year or landscapes. Our results suggest that, for mobile species within recently fragmented habitat, habitat loss after disturbance may lead to local population extinction but may augment genetic diversity in remnant local populations because of increased gene flow.},
  date-added = {2016-02-09T09:57:55GMT},
  date-modified = {2016-02-09T09:58:19GMT},
  rating = {0},
  uri = {papers3://publication/uuid/A268C21E-A6BF-4E5E-A15C-9242DBABE26C}
}

@article{Fayle:2009vb,
  title = {The Effect of Rain Forest Canopy Architecture on the Distribution of Epiphytic Ferns (Asplenium Spp.) in Sabah, Malaysia},
  author = {Fayle, Tom M and Chung, Arthur Y C and Dumbrell, Alex J and Eggleton, Paul and Foster, William A},
  year = {2009},
  journal = {BIOTROPICA},
  volume = {41},
  number = {6},
  pages = {676--681},
  abstract = {Epiphytic plants are a dominant component of the rain forest canopy biota. They represent a significant proportion of canopy plant biomass and diversity, play a key role in nutrient cycling, and support highly abundant and diverse animal communities. Understanding the factors affecting their distribution in this three-dimensional habitat is consequently of great importance, not least because they may be particularly vulnerable to climate change and habitat conversion. Here we investigate how canopy architecture affects the distribution of two species of bird's nest fern (Asplenium spp.) in pristine rain forest. Both species were found at high abundances (Asplenium phyllitidis: 136/ha, SE +/- 31.6, Asplenium nidus: 44/ha, SE +/- 9.2) and their distributions were differentially affected by canopy architecture. Asplenium phyllitidis was found only at heights \textexclamdown{} 30 m in areas with a thicker lower canopy layer. Asplenium nidus was found at all heights in the canopy and was associated with emergent trees and areas with an open understory. Larger A. phyllitidis were found higher in the canopy while larger A. nidus were found on trunks and branches with a wider diameter. Asplenium nidus seems adapted to withstand the hot dry conditions in the upper canopy and in gaps, and its size is consequently limited only by the size of its support. Asplenium phyllitidis is dominant in areas that are cooler and damper, and so the growth rate of individuals may be limited by light levels. We discuss possible implications of this partitioning for epiphyte communities in the face of climate change and habitat conversion.},
  date-added = {2016-02-09T09:57:56GMT},
  date-modified = {2016-02-09T09:58:19GMT},
  rating = {0},
  uri = {papers3://publication/uuid/B60ADF41-2B9B-4050-A66C-F649484662A6}
}

@misc{fayleHowDoesForest2019,
  title = {How Does Forest Conversion and Fragmentation Affect Ant Communities and the Ecosystem Processes That They Mediate?},
  author = {Fayle, Tom M and Yusah, Kalsum M and Ewers, Robert M and Boyle, Michael J W},
  year = {2019},
  publisher = {{Zenodo}},
  abstract = {\textexclamdown b\textquestiondown Description: \textexclamdown/b\textquestiondown Data collected by Tom M. Fayle and Kalsum M. Yusah, Robert M. Ewers was the principal investigator\textexclamdown br\textquestiondown With the assistance of Oli Ashford, Emma Garnet, Imogen Ogilvie and Ed Turner\textexclamdown br\textquestiondown Uploaded by Michael J. W. Boyle\textexclamdown br\textquestiondown Data collected on SAFE sites in Benta and at Maliau between 29/06/2012 to 14/12/2011\textexclamdown br\textquestiondown Abundances are observations of numbers of ants entering a 12 cm by 14 cm plastic card, laid flat in the leaf litter, baited with 30 compressed dried earthworm pellets of a range of sizes over a 40 minute period\textexclamdown br\textquestiondown Collections were made from two of each of the triplets of first order sampling points. All triplets were visited, with the exception of the oil palm, logged forest and primary forest controls, where 2/3 of the triplets were visited.\textexclamdown br\textquestiondown Species were identified from vouchers, and vouchers were not collected for all species at all sites, hence the unidentified species (these can be used to calculate total ant abundance and species richness at a bait card, but not total species richness or anything involving trophic levels or sizes). 98\% of ant individuals observed and 93\% of ant occurrences had an associated voucher collected\textexclamdown br\textquestiondown\textexclamdown br\textquestiondown There are six files that together contain all the ant bait removal data: ""ant attendance"", ""ant attendance summary"", ""bait locations"", ""bait removals"", ""ant IDs"" and ""morphospecies""\textexclamdown br\textquestiondown\textexclamdown br\textquestiondown For ""ant attendence""\textexclamdown br\textquestiondown ""Site"" refers to SAFE sampling site location\textexclamdown br\textquestiondown ""Species"" refers to the ant species number within that site (ranges from 1 up to 2 or 3, occasionally more)\textexclamdown br\textquestiondown ""Time.Period"" refers to the start time of the five minute period for which ant attendance at the bait was recorded.\textexclamdown br\textquestiondown ""Abundance"" refers to the abundance within that five minute period.\textexclamdown br\textquestiondown Note that only non-zero abundances are recorded. For all time period*species*sites combinations not listed, the abundance is zero\textexclamdown br\textquestiondown\textexclamdown br\textquestiondown For ""ant attendance summary""\textexclamdown br\textquestiondown ""Site"" refers to SAFE sampling site location\textexclamdown br\textquestiondown ""Date"" is date of field sampling\textexclamdown br\textquestiondown ""Block"" is the SAFE sampling area\textexclamdown br\textquestiondown Columns then refer to morphospecies numbers (following identification in the lab), with entries being totals of ants entering the bait cards over the entire 40 minute period. This has been generated using the ""ant IDs"" in combination with the ""ant attendance"" data\textexclamdown br\textquestiondown\textexclamdown br\textquestiondown For ""bait locations""\textexclamdown br\textquestiondown The rows relate to the ""Sheet number"" column in the ""bait removals"" file.\textexclamdown br\textquestiondown The columns relate to the thirty (5*6) bait locations on the bait card, specifically in relation to the numbers in the ""bait removals"" file.\textexclamdown br\textquestiondown The entries relate to the bait sizes, 1=0.125, 2=0.250, 3=0.500, 4=1.000\textexclamdown br\textquestiondown\textexclamdown br\textquestiondown For ""bait removals""\textexclamdown br\textquestiondown There are a number of environmental and other card specific variables recorded, all should be self explanatory.\textexclamdown br\textquestiondown In addition, for each of the bait locations, there is the information of which ant species removed it and at what time (e.g. ""S1"" and ""T1"" for bait number 1), and also the first time of arrival of each species (on the entire bait card) and a brief description\textexclamdown br\textquestiondown\textexclamdown br\textquestiondown For ""ant IDs""\textexclamdown br\textquestiondown ""Identified.by"" is the person who identified the samples\textexclamdown br\textquestiondown ""Order"" is the order the samples were processed in (can help with any error checking)\textexclamdown br\textquestiondown ""Site"" refers to SAFE sampling site location\textexclamdown br\textquestiondown ""Date"" is date of field sampling\textexclamdown br\textquestiondown ""Block"" is the SAFE sampling area\textexclamdown br\textquestiondown ""ant.sp.on.sheets"" refers to the ant species number WITHIN a dataset, i.e. it only goes up to 3 or 4 (corresponds to ""Species"" in file ""ant attendance"")\textexclamdown br\textquestiondown ""sp.description"" field description of species, variably in Malay, English or latin, depending on who was observing\textexclamdown br\textquestiondown ""Count"" number of individuals collected and identified\textexclamdown br\textquestiondown Then remaining columns are morphospecies numbers (see file ""morphospecies"") with entries in cells being the number of individuals collected and identified.\textexclamdown br\textquestiondown\textexclamdown br\textquestiondown For ""morphospecies""\textexclamdown br\textquestiondown ""Order for IDs"" is the order morphospecies were accumulated - was useful for error checking\textexclamdown br\textquestiondown ""Morph. No"" the morphospecies number, corresponds to columns in ""ant attendance summary"" and ""ant IDs""\textexclamdown br\textquestiondown ""Genus"" the genus of ant - note that the IDs were done a few years ago and taxonomy has not been updated\textexclamdown br\textquestiondown ""Description"" description of morphospecies to help distinguish it from others in the same genus\textexclamdown br\textquestiondown ""Weber's length (mm)"" Sizes as Weber's length, the maximum length of the ant alitrunk, measured using a graticule/binocular microscope and averaged from between 1 and 5 minor workers from each species\textexclamdown br\textquestiondown ""Delta N"" trophic data - see below\textexclamdown br\textquestiondown ""Notes"" noted cases where trophic data were from a different source\textexclamdown br\textquestiondown ""N"" number of individuals measured for Weber's length (most commonly n=1)\textexclamdown br\textquestiondown\textexclamdown br\textquestiondown Trophic data are enrichment levels of Nitrogen isotopes - higher values indicate that a species is more predatory, lower that it is more hebivorous. Most data are averages at the genus level from Woodcock et al. (2013) and Pfeiffer et al. (2013). For data from Woodcock et al (2013), a mean was taken for measurements from the two habitats - primary forest and logged forest. For genera represented in both studies (Woodcock and Pfeiffer), a mean was taken for the values from both\textexclamdown br\textquestiondown However, where there was no genus-level data available from these publications on litter ants, data were used from canopy samples from Davidson et al. (2003) and Bluthgen et al. (2003). Where this was done it is noted\textexclamdown br\textquestiondown For a few genera there are no trophic data at all.\textexclamdown br\textquestiondown\textexclamdown br\textquestiondown Sources for trophic data\textexclamdown br\textquestiondown Bl\"uthgen, N., G. Gebauer, and K. Fiedler. 2003. Disentangling a rainforest food web using stable isotopes: dietary diversity in a species-rich ant community. Oecologia 137:426-435.\textexclamdown br\textquestiondown Davidson, D. W., S. C. Cook, R. R. Snelling, and T. H. Chua. 2003. Explaining the abundance of ants in lowland tropical rainforest canopies. Science 300:969-972.\textexclamdown br\textquestiondown Pfeiffer, M., D. Mezger, and J. Dyckmans. 2013. Trophic ecology of tropical leaf litter ants (Hymenoptera: Formicidae) \textendash{} a stable isotope study in four types of Bornean rain forest. Myrmecological News 19:31-41.\textexclamdown br\textquestiondown Woodcock, P., D. P. Edwards, R. J. Newton, C. Vun Khen, S. H. Bottrell, and K. C. Hamer. 2013. Impacts of Intensive Logging on the Trophic Organisation of Ant Communities in a Biodiversity Hotspot. PLoS ONE 8:e60756."\textexclamdown b\textquestiondown Project: \textexclamdown/b\textquestiondown This dataset was collected as part of the following SAFE research project: \textexclamdown b\textquestiondown How does forest conversion and fragmentation affect ant communities and the ecosystem processes that they mediate?\textexclamdown/b\textquestiondown\textexclamdown b\textquestiondown Funding: \textexclamdown/b\textquestiondown These data were collected as part of research funded by: Sime Darby Foundation (Grant)This dataset is released under the CC-BY 4.0 licence, requiring that you cite the dataset in any outputs, but has the additional condition that you acknowledge the contribution of these funders in any outputs.\textexclamdown b\textquestiondown Permits: \textexclamdown/b\textquestiondown These data were collected under permit from the following authorities:Sabah Biodiversity Centre (Research licence NA)\textexclamdown b\textquestiondown XML metadata: \textexclamdown/b\textquestiondown GEMINI compliant metadata for this dataset is available here\textexclamdown b\textquestiondown Files: \textexclamdown/b\textquestiondown This consists of 1 file: Ant\_Bait\_Removal\_SAFE\_database.xlsx\textexclamdown b\textquestiondown Ant\_Bait\_Removal\_SAFE\_database.xlsx\textexclamdown/b\textquestiondown This file contains dataset metadata and 6 data tables:\textexclamdown b\textquestiondown Ant Attendance\textexclamdown/b\textquestiondown{} (described in worksheet Ant\_Attendance)Description: Ant AttendanceNumber of fields: 6Number of data rows: 8186Fields: \textexclamdown b\textquestiondown Order\textexclamdown/b\textquestiondown : Order in which samples processed (Field type: Numeric)\textexclamdown b\textquestiondown Site\textexclamdown/b\textquestiondown : SAFE order point (Field type: Numeric)\textexclamdown b\textquestiondown Location\textexclamdown/b\textquestiondown : Location within the SAFE landscape (Field type: Location)\textexclamdown b\textquestiondown Taxa\textexclamdown/b\textquestiondown : refers to the ant species number within that site (ranges from 1 up to 2 or 3, occasionally more) (Field type: Taxa)\textexclamdown b\textquestiondown Time\_period\textexclamdown/b\textquestiondown : refers to the start time of the five minute period for which ant attendance at the bait was recorded (Field type: Numeric)\textexclamdown b\textquestiondown Abundance\textexclamdown/b\textquestiondown : Abundance within selected 5 minute period (Field type: Abundance)\textexclamdown b\textquestiondown Ant Attendance Summary\textexclamdown/b\textquestiondown{} (described in worksheet Ant\_Attendance\_Summary)Description: Ant Attendance SummaryNumber of fields: 274Number of data rows: 332Fields: \textexclamdown b\textquestiondown SAFE\_location\textexclamdown/b\textquestiondown : Location within the SAFE landscape (Field type: Location)\textexclamdown b\textquestiondown Date\textexclamdown/b\textquestiondown : Date (Field type: Date)\textexclamdown b\textquestiondown Block\textexclamdown/b\textquestiondown : SAFE fragment (Field type: Categorical)\textexclamdown b\textquestiondown sp\_1\textexclamdown/b\textquestiondown : Total number of ants visiting card, column name refers to morphospecies ID (Field type: Abundance)\textexclamdown b\textquestiondown sp\_2\textexclamdown/b\textquestiondown : Total number of ants visiting card, column name refers to morphospecies ID (Field type: Abundance)\textexclamdown b\textquestiondown sp\_3\textexclamdown/b\textquestiondown : Total number of ants visiting card, column name refers to morphospecies ID (Field type: Abundance)\textexclamdown b\textquestiondown sp\_4\textexclamdown/b\