  pages = {243--248},
  doi = {10.1038/s41586-018-0189-9},
  abstract = {Explaining the large-scale diversity of soil organisms that drive biogeochemical processes\textemdash and their responses to environmental change\textemdash is critical. However, identifying consistent drivers of belowground diversity and abundance for some soil organisms at large spatial scales remains problematic. Here we investigate a major guild, the ectomycorrhizal fungi, across European forests at a spatial scale and resolution that is\textemdash to our knowledge\textemdash unprecedented, to explore key biotic and abiotic predictors of ectomycorrhizal diversity and to identify dominant responses and thresholds for change across complex environmental gradients. We show the effect of 38 host, environment, climate and geographical variables on ectomycorrhizal diversity, and define thresholds of community change for key variables. We quantify host specificity and reveal plasticity in functional traits involved in soil foraging across gradients. We conclude that environmental and host factors explain most of the variation in ectomycorrhizal diversity, that the environmental thresholds used as major ecosystem assessment tools need adjustment and that the importance of belowground specificity and plasticity has previously been underappreciated.},
  date-added = {2018-06-22T08:38:15GMT},
  date-modified = {2021-02-26T14:03:12GMT},
  langid = {english},
  local-url = {file://localhost/Users/dorme/References/Library.papers3/Articles/2018/Van\%20Der\%20Linde/Nature\%202018\%20Van\%20Der\%20Linde-2.pdf},
  rating = {0},
