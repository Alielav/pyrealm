@book{Yumoto:1996ti,
  title = {Monitoring and Inventorying System in Canopy Biology Program in {{Sarawak}}, {{Malaysia}}.},
  author = {Yumoto, T and Inoue, T and Hamid, A A},
  year = {1996},
  abstract = {Mixed dipterocarp forests in Sarawak are the richest tropical rain forests in the world with high diversity in tree species. Unstable, global environmental changes such as the El Nino Southern Oscillation have a strong influence on tropical rain forests of Sarawak as well as other regions of Southeast Asia. Research in the Canopy Biology Program in Sarawak, aims to clarify firstly that unstable environmental changes at the global level influence phenology and reproductive systems of trees studied, and secondly, that life history strategy and population dynamics of animals that build mutualistic relationships with plants and feed on materials produced by plants which are affected by the environmental changes directly or indirectly through plant phenology. Plant reproduction mostly happens in the forest canopy, and to study this, two tree towers and nine spans of walkway were constructed in Lambir Hills National Park in Sarawak, Malaysia, to observe plant phenology and plant-animal interactions. Reproductive and leaf flushing phenology of over 500 plant individuals and the seasonal changes of insect abundance were monitored for 3 years. This paper aims to establish the baseline data collection for the study of the Canopy Biology Program in Sarawak.},
  date-added = {2016-02-09T10:03:16GMT},
