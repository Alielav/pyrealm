@article{Huth:2000wn,
  title = {Simulation of the Growth of a Lowland {{Dipterocarp}} Rain Forest with {{FORMIX3}}},
  author = {Huth, A and Ditzer, T},
  year = {2000},
  journal = {Ecological Modelling},
  volume = {134},
  number = {1},
  pages = {1--25},
  abstract = {In this paper a new model for simulation of the growth of tropical rain forest is presented (FORMIX3). The model describes growth, mortality, recruitment of trees and competition between trees. The calculation of tree growth is based on a carbon balance. The carbon gain of a tree depends on the photo production of its leaves, respiration and other losses. Trees compete for light and space. Dying large trees fall down and create gaps in the forest. Based on an extensive field data review, a parametrisation was developed for the simulation of lowland Dipterocarp rain forest at Deramakot, Malaysia. A total of 28 variables describing different aspects of forest structure and growth were compared with field data. The model reproduces most parts of the forest dynamics well. A new concept for sensitivity analysis is presented: 46 model parameter were varied and analysed in respect to their influence on 26 variables describing the forest state. The influence of the different processes on forest structure is complex. Some general trends can be observed: The growth characteristics of the two Dipterocarp species groups strongly influences species composition in the forest, but not general forest structure (biomass, leaf area, production, leaf area, tree size distribution)},
  date-added = {2018-05-10T13:48:34GMT},
  date-modified = {2018-07-10T11:59:12GMT},
  rating = {0},
  uri = {papers3://publication/uuid/2B604740-0931-49A6-B5E8-F853A51EE20A}
}

@article{Huth:2005bh,
  title = {Using Multicriteria Decision Analysis and a Forest Growth Model to Assess Impacts of Tree Harvesting in {{Dipterocarp}} Lowland Rain Forests},
  author = {Huth, Andreas and Drechsler, Martin and K{\"o}hler, Peter},
  year = {2005},
  journal = {FOREST ECOLOGY AND MANAGEMENT},
  volume = {207},
  number = {1},
  pages = {215--232},
  publisher = {{Elsevier}},
  doi = {10.1016/j.foreco.2004.10.028},
  abstract = {Sixty-four different tree-harvesting scenarios in an initially undisturbed Dipterocarp lowland rain forest stand in Sabah (Malaysia) were simulated with the rain forest growth model FORMIND. The scenarios differ in terms of their minimum cutting diameter, logging cycle, method and intensity. The simulation results include harvest yields and the impact on forest structure (canopy opening and changes in species composition). Multicriteria decision analysis was used to evaluate the scenarios and identify optimum ones by applying a stochastic extension of the PROMETHEE method. Almost all optimum scenarios used reduced-impact logging. High cutting limits or low logging intensities could not compensate for the high damage caused by conventional logging techniques. Five scenarios proved to be optimum for a wide range of priorities concerning different forest functions. They all use reduced-impact logging and long logging cycles (60 years), either with a minimum cutting limit of 50 or 60cm stem diameter, or with medium logging intensities.},
  date-added = {2018-05-10T13:51:01GMT},
  date-modified = {2018-07-10T11:59:12GMT},
  langid = {english},
  rating = {0},
  read = {Yes},
  uri = {papers3://publication/doi/10.1016/j.foreco.2004.10.028}
}

@article{Ichie:2013uw,
  title = {Dynamics of Mineral Nutrient Storage for Mast Reproduction in the Tropical Emergent Tree {{Dryobalanops}} Aromatica},
  author = {Ichie, Tomoaki and Nakagawa, Michiko},
  year = {2013},
  journal = {ECOLOGICAL RESEARCH},
  volume = {28},
  number = {2},
  pages = {151--158},
  abstract = {Limitations in resources are considered to be important in mass-fruiting trees, based on the speculation that they need to consume a large amount of stored resources in a very short period. To test this hypothesis, we studied the total reproductive costs per individual of the tree Dryobalanops aromatica (Dipterocarpaceae), and the dynamics of stored mineral nutrients, specifically nitrogen, phosphorus, and potassium; we then estimated the contribution of each of the stored resources to reproduction in a lowland dipterocarp forest in Sarawak, Malaysia. D. aromatica is an emergent and masting tree species, and it produced a large amount of mature fruit in a masting period running 2001-2002. The dynamics patterns were markedly different for each of the stored nutrients. The phosphorus concentration clearly decreased in the branch, stem and root during reproduction, but nitrogen fluctuated by only a small amount in any organ during this period. The potassium concentration fell in all organs during flowering, but even increased during fruit development. Stored phosphorus accounted for 67.7\% of the total phosphorus requirements for reproduction, but nitrogen accounted for only 19.7\%. Stored potassium did not appear to contribute anything, at least to fruit production. Our results suggest that D. aromatica supplies phosphorus needed for reproduction mainly from stored resources in the tree, unlike nitrogen or potassium. The accumulation of phosphorus could be the decisive factor in the occurrence and frequency of mast reproduction in the relatively poor soil conditions prevailing in the tropical rain forests of Southeast Asia.},
  date-added = {2016-02-09T09:55:58GMT},
  date-modified = {2016-02-09T09:58:17GMT},
  rating = {0},
  uri = {papers3://publication/uuid/167F209E-0D16-495B-8E4B-A8A1BE3E5124}
}

@misc{ImpactOfElNinoDr:2018te,
  title = {Impact of El Nino Drought on Seedling Dynamics},
  author = {Qie, Lan and Telford, Elizabeth and Massam, Michael and Ewers, Robert},
  year = {2018},
  publisher = {{Zenodo}},
  abstract = {\textexclamdown b\textquestiondown Description: \textexclamdown/b\textquestiondown Four SAFE seedling plot censuses including measurement after the 2015-16 El Nino drought and leaf trait data\textexclamdown b\textquestiondown Project: \textexclamdown/b\textquestiondown This dataset was collected as part of the following SAFE research project: \textexclamdown b\textquestiondown Resilience of Tropical Forest Ecosystem Processes to the Interactive Effects of El Nino and Forest Disturbance\textexclamdown/b\textquestiondown\textexclamdown b\textquestiondown XML metadata: \textexclamdown/b\textquestiondown GEMINI compliant metadata for this dataset is available here\textexclamdown b\textquestiondown Files: \textexclamdown/b\textquestiondown This consists of 1 file: Seedling\_data\_for\_SAFE\_upload\_2\_Aug\_2018.xlsx\textexclamdown b\textquestiondown Seedling\_data\_for\_SAFE\_upload\_2\_Aug\_2018.xlsx\textexclamdown/b\textquestiondown This file contains dataset metadata and 3 data tables:\textexclamdown b\textquestiondown SAFE seedling plot censuses\textexclamdown/b\textquestiondown{} (described in worksheet Censuses)Description: In each of the 84 SAFE vegetation plot a 5 m x 5 m seedling plot was established in 2012. All tree and liana seedlings  \&gt;= 50 cm height and  \&lt;= 1 cm DBH were tagged mapped and identified. At the centre of the 5 m x 5 m plot a subplot of 2 m x 2 m was marked out where all seedlings  \&gt;= 10 cm height were included. Censuses 1 \&amp;2 in 2012 were carried out by Hamzah Bin Tangki (PhD thesis 2014); census 3 in 2015 was carried out by Michael Massam (MSc thesis 2015); census 4 in 2017 was carried out by Elizabeth Telford. Diameter was measured at 20 cm from ground. Height was measured for tree seedlings (not measured if stem was snapped). A number of stems are missing diameter and height records in earlier censuses but considered alive at those times based on information from census 4 \textendash{} these data rows are kept and NA values assigned. Species id were done by SAFE field botanists. 2017 recruits stems have less complete identification. 11 seedling plots were not measured due to disturbances, mainly logging road and landslide.Number of fields: 12Number of data rows: 7376Fields: \textexclamdown b\textquestiondown census\textexclamdown/b\textquestiondown : census number (Field type: ID)\textexclamdown b\textquestiondown Plot\textexclamdown/b\textquestiondown : SAFE project plot number (Field type: Location)\textexclamdown b\textquestiondown tag\textexclamdown/b\textquestiondown : seedling tag number (Field type: ID)\textexclamdown b\textquestiondown North\textexclamdown/b\textquestiondown : coordinate within seedling plot: northing (Field type: Numeric)\textexclamdown b\textquestiondown East\textexclamdown/b\textquestiondown : coordinate within seedling plot: easting (Field type: Numeric)\textexclamdown b\textquestiondown type\textexclamdown/b\textquestiondown : growth form of seedling: tree or vine (liana) (Field type: Categorical)\textexclamdown b\textquestiondown species\textexclamdown/b\textquestiondown : species (Field type: Taxa)\textexclamdown b\textquestiondown Da\textexclamdown/b\textquestiondown : diameter measurement 1 (Field type: Numeric)\textexclamdown b\textquestiondown Db\textexclamdown/b\textquestiondown : diameter measurement 2 (Field type: Numeric)\textexclamdown b\textquestiondown height\textexclamdown/b\textquestiondown : seedling height (Field type: Numeric)\textexclamdown b\textquestiondown snapped\textexclamdown/b\textquestiondown : stem snapped or not (Field type: Categorical)\textexclamdown b\textquestiondown remark\textexclamdown/b\textquestiondown : remark (Field type: Comments)\textexclamdown b\textquestiondown Germination count\textexclamdown/b\textquestiondown{} (described in worksheet Germination)Description: At census 3 and 4, newly germinated seedlings were counted for the 2 m x 2 m seedling subplot. These were not tagged, mapped or identified.Number of fields: 4Number of data rows: 168Fields: \textexclamdown b\textquestiondown Plot\textexclamdown/b\textquestiondown : SAFE project plot number (Field type: Location)\textexclamdown b\textquestiondown census\textexclamdown/b\textquestiondown : census number (Field type: ID)\textexclamdown b\textquestiondown count\textexclamdown/b\textquestiondown : count of newly germinated seedlings (Field type: Abundance)\textexclamdown b\textquestiondown remark\textexclamdown/b\textquestiondown : remark (Field type: Comments)\textexclamdown b\textquestiondown Seedling leaf traits\textexclamdown/b\textquestiondown{} (described in worksheet Leaf traits)Description: At census 4, for each seedling in the plots which was deemed big enough, the fifth youngest leaf counting from the top shoot was collected. If damaged, the nearest alternative was taken Leaf petiole was removed and leaf was photographed while fresh. Fresh weight and oven dry weight were measured. Leaf area was calculated from photographs using ImageJ. These measurements were used to calculate seedling CSR trait values (Piece, S., et al. 2017 Functional Ecology)Number of fields: 5Number of data rows: 1072Fields: \textexclamdown b\textquestiondown Plot\textexclamdown/b\textquestiondown : SAFE project plot number (Field type: Location)\textexclamdown b\textquestiondown tag\textexclamdown/b\textquestiondown : seedling tag number (Field type: ID)\textexclamdown b\textquestiondown Fresh.Weight\textexclamdown/b\textquestiondown : leaf fresh weight (Field type: Numeric)\textexclamdown b\textquestiondown Dry.Weight\textexclamdown/b\textquestiondown : leaf dry weight (Field type: Numeric)\textexclamdown b\textquestiondown Leaf.Area\textexclamdown/b\textquestiondown : leaf area (Field type: Numeric)\textexclamdown b\textquestiondown Date range: \textexclamdown/b\textquestiondown 2012-06-01 to 2017-12-31\textexclamdown b\textquestiondown Latitudinal extent: \textexclamdown/b\textquestiondown 4.6882 to 4.7714\textexclamdown b\